  "version": "0.2.0",
  "configurations": [
    {
      "type": "lldb",
      "request": "launch",
      "name": "Debug executable 'geno'",
      "cargo": {
        "args": ["build", "--bin=geno", "--package=geno"],
        "filter": {
          "name": "geno",
          "kind": "bin",
        },
      },
      "args": ["${workspaceFolder}/examples/bad_struct.geno"],
      "program": "${workspaceFolder}/target/debug/geno",
      "cwd": "${workspaceFolder}",
      "stopOnEntry": false,
      "sourceLanguages": ["rust"],
    },
  ],
}
